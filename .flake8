# This is an example .flake8 config, used when developing *Black* itself.
# Keep in sync with setup.cfg which is used for source packages.

[flake8]
# W606: reserved keywords
ignore = W292,F403,F405,C901,E741
max-line-length = 130
max-complexity = 18
select = B,C,E,F,W,T4,B9
exclude = ./dipoorlet/proto
per-file-ignores = */__init__.py:F401, ./dipoorlet/deploy/deploy_base.py:F401, ./dipoorlet/tensor_cali/tensor_cali_base.py:F401 ./dipoorlet/main.py:E402
